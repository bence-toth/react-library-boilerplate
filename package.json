{
  "name": "react-library-boilerplate",
  "version": "1.0.0",
  "description": "Feature-rich boilerplate for React libraries",
  "keywords": [
    "react",
    "boilerplate"
  ],
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "webpack --mode production",
    "build:storybook": "build-storybook -c .storybook -o ./.storybook/dist",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:gui": "majestic",
    "test:coverage": "jest --coverage",
    "lint": "npm run eslint ; npm run stylelint ; npm run proofread",
    "eslint": "eslint --ignore-path .gitignore --ext .jsx --ext .js .",
    "stylelint": "stylelint \"./src/**/*.css\"",
    "proofread": "alex",
    "update": "npm-check -u"
  },
  "author": {
    "name": "Bence A. Toth",
    "email": "tothab@gmail.com"
  },
  "homepage": "https://github.com/bence-toth/react-library-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bence-toth/react-library-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/bence-toth/react-library-boilerplate/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@bence.a.toth/eslint-config": "^1.0.14",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-backgrounds": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "alex": "^8.1.1",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "majestic": "^1.6.2",
    "npm-check": "^5.9.2",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "prop-types": ">= 15.7.2",
    "react": ">= 16.12.0",
    "react-dom": ">= 16.12.0"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  },
  "main": "dist/main.js",
  "files": [
    "dist/main.js",
    "README.md",
    "LICENSE",
    "changelog.md"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
